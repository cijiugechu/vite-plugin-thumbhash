{
  "name": "vite-plugin-thumbhash",
  "version": "0.1.6",
  "description": "Add ThumbHash to your vite project.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": false,
  "keywords": [
    "vite",
    "thumbhash",
    "vite-plugin",
    "plugin",
    "thumb",
    "image",
    "avif"
  ],
  "files": [
    "dist",
    "client.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "scripts": {
    "build:debug": "tsup --config tsup.debug.ts",
    "build": "tsup --config tsup.config.ts",
    "bench": "node ./bench.mjs"
  },
  "homepage": "https://github.com/cijiugechu/vite-plugin-thumbhash",
  "repository": {
    "type": "git",
    "url": "git@github.com:cijiugechu/vite-plugin-thumbhash.git"
  },
  "bugs": "https://github.com/cijiugechu/vite-plugin-thumbhash/issues",
  "author": "nemurubaka",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "benny": "^3.7.1",
    "thumbhash": "^0.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "@napi-rs/canvas": "^0.1.41",
    "thumbhash-node": "^0.1.1"
  },
  "peerDependencies": {
    "vite": ">=2.9.5"
  }
}
